{
	"name": "Video queue",
	"description": "Queue video's of that one site for an instant playlist",
	"version": "1.12",

	"content_scripts": [ {
		"all_frames": true,
		"js": [ "youtubequeue.content.js" ],
		"css": [ "youtubequeue.content.css" ],
		"matches": [ "*://youtube.com/*", "*://www.youtube.com/*" ],
		"run_at": "document_idle"
	} ],

	"icons": {
		"128": "images/icon128.png",
		"48": "images/icon48.png"
	},
	"manifest_version": 2
}
